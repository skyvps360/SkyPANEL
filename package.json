{
  "name": "skypanel",
  "version": "14.0.0",
  "type": "module",
  "license": "MIT",
  "description": "SkyPANEL - A modern web panel for managing your servers and applications.",
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx server/index.ts",
    "dev:clean": "npm run clean && npm run dev",
    "clean": "rimraf node_modules/.vite",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "build:restart": "npm run build && pm2 delete all && pm2 start pm2.config.cjs",
    "build:start": "npm run build && pm2 start pm2.config.cjs",
    "build-restart:pm2:windows": "npm run build && pm2 delete all && npm run start:pm2:windows",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "start:pm2:linux": "pm2 start pm2.config.cjs",
    "start:pm2:windows": "pm2 start dist/index.js --name skypanel --node-args \"\" --max-memory-restart 2G",
    "stop:pm2": "pm2 stop skypanel",
    "restart:pm2": "pm2 restart skypanel",
    "logs:pm2": "pm2 logs skypanel",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "update-packages": "ncu -u",
    "verify-env": "tsx scripts/verify-env.js",
    "health-check": "curl -f http://localhost:3333/api/health || exit 1",
    "test": "vitest run",
    "test:watch": "vitest"
  },
